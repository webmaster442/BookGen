<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Product>BookGen</Product>
		<OutputPath>..\..\bin\$(Configuration)\</OutputPath>
		<Nullable>enable</Nullable>
